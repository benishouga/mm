{
  "name": "mm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "format": "prettier --parser typescript --write  \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --content-base public --hot --open",
    "dev": "concurrently \"npm run emulator\" \"npm start\"",
    "emulator": "firebase emulators:start --only database --import=./.firebase_emulator_data --export-on-exit",
    "deploy": "npm run build && firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benishouga/mm.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benishouga/mm/issues"
  },
  "homepage": "https://github.com/benishouga/mm#readme",
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "firebase-tools": "^9.2.2",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/recoil": "0.0.1",
    "firebase": "^8.2.10",
    "firebase-admin": "^9.2.0",
    "immer": "^7.0.5",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "recoil": "^0.1.2",
    "uuid": "^8.1.0"
  }
}
