    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^8.0.0",
    "@types/recoil": "0.0.1",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
