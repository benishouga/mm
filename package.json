{
  "name": "mm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "format": "prettier --parser typescript --write  \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack serve",
    "dev": "concurrently \"npm run emulator\" \"npm start\"",
    "emulator": "firebase emulators:start --only database --import=./.firebase_emulator_data --export-on-exit",
    "deploy": "npm run build && firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benishouga/mm.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benishouga/mm/issues"
  },
  "homepage": "https://github.com/benishouga/mm#readme",
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "firebase-tools": "^14.20.0",
    "prettier": "^2.0.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@types/recoil": "0.0.1",
    "firebase": "^12.4.0",
    "immer": "^10.1.3",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "recoil": "^0.1.2",
    "uuid": "^8.1.0"
  },
  "overrides": {
    "brace-expansion": "1.1.12",
    "color-string": "1.9.1",
    "compression": "1.8.1",
    "debug": "4.3.4",
    "minimist": "1.2.8",
    "minimatch": "3.1.2",
    "on-headers": "1.1.0",
    "semver": "6.3.1",
    "websocket-extensions": "0.1.4"
  }
}
